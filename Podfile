# platform :ios, '9.0'

target 'Builder Notifications' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
    pod 'Firebase', '4.0.3'
    pod 'Firebase/Core'
    pod 'Firebase/Database'
    pod 'Firebase/Messaging'
    pod 'Firebase/Auth'
    pod 'leveldb-library'
    pod 'leveldb'

  # Pods for Builder Notifications

  target 'Builder NotificationsTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Builder NotificationsUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
